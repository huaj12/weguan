<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry location="/var/root/.m2/repository/mysql/mysql-connector-java/5.1.12/mysql-connector-java-5.1.12.jar" />

  <context id="MySQLTables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://127.0.0.1:3306/weguan"
        userId="root"
        password="root">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.weguan.passport.model" targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
      <!-- <property name="rootClass" value="java.io.Serializable"/> -->
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="mybatis"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.weguan.passport.dao"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <table tableName="tb_example" domainObjectName="Example" >
      <!-- <property name="useActualColumnNames" value="true"/> -->
      <!-- <generatedKey column="ID" sqlStatement="DB2" identity="true" />
      <columnOverride column="DATE_FIELD" property="startDate" />
      <ignoreColumn column="FRED" />
      <columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR" /> -->
    </table>

  </context>
</generatorConfiguration>